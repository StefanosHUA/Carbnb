# Google OAuth Configuration
# Replace these with your actual Google OAuth credentials
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id-here
REACT_APP_GOOGLE_CLIENT_SECRET=your-google-client-secret-here
REACT_APP_GOOGLE_REDIRECT_URI=http://localhost:3000

# API Configuration
# Main API Gateway URL (if using API Gateway pattern)
REACT_APP_API_URL=http://localhost:3001

# Microservice URLs (if services run on separate ports)
REACT_APP_USER_SERVICE_URL=http://localhost:8002
REACT_APP_VEHICLE_SERVICE_URL=http://localhost:3003

# Development Configuration
REACT_APP_ENV=development 